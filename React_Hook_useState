import React, { useState } from 'react';
import './HomePage.css';

export default function HomePage() {
    const [count, setCount] = useState(0);

    const increment = () => {
        setCount(count + 1);
    };

    return (
        <>  
            <h1>Page d'accueil</h1>
            <p>compteur: {count}</p>
            <button onClick={increment}>incrementer</button>
        </>
    );
}
